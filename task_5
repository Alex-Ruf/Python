my_list = [7, 5, 3, 3, 2]
number = input("Введите число")
while not number.isdigit():
    number = input("Введите число")

if my_list[-1] >= int(number):
    my_list.append(number)
    print(my_list)

else:
    for i in my_list:
        if int(number) > int(i):
            my_list.insert(my_list.index(i), str(number))
            print(my_list)
            break
