class Road():

    def __init__(self, length, width, height):
        self.__length = length
        self.__width = width
        self.__height = height
        self.__netto = 25

    def result(self):
        print("Total(tonn): " + str((self.__length * self.__height * self.__width * self.__netto) / 1000))


road1 = Road(5000, 20, 5)
road1.result()
